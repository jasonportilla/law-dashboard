{
  "extends": [
    "airbnb",
    "react-app"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "jsx-a11y"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "jsx-a11y/href-no-hash": [
      0
    ],
    "react/forbid-prop-types": 0
  }
}